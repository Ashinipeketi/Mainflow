
import pandas as pd 
# reading the database 
data pd.read_csv("tips .csv" ) 
# print ing the top 10 rows 
print(data.head(10))
# reading the database 

plt.scatter(data['day'], data['tip'], c=data['size'], 

            s=data['total_bill'])
 
# Adding Title to the Plot

plt.title("Scatter Plot")
 
